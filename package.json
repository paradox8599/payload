{
  "name": "blank-payload",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prepare": "git config core.hooksPath .git-hooks 2>/dev/null",
    "dev": "next dev",
    "devsafe": "rm -rf .next && next dev",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "gen": "bun generate:importmap && bun generate:types",
    "build": "next build",
    "start": "next start",
    "build:docker": "docker build -t pl --secret id=env,src=.env .",
    "deploy:fly": "fly deploy --build-secret env=\"$(cat .env)\"",
    "lint": "next lint",
    "payload": "payload",
    "clean": "rm -rf .next node_modules"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.2.1",
    "@payloadcms/db-vercel-postgres": "^3.2.1",
    "@payloadcms/email-resend": "^3.2.1",
    "@payloadcms/next": "^3.2.1",
    "@payloadcms/richtext-lexical": "^3.2.1",
    "@payloadcms/storage-s3": "^3.2.1",
    "@payloadcms/ui": "^3.2.1",
    "graphql": "^16.9.0",
    "next": "^15.0.3",
    "payload": "^3.2.1",
    "react": "^19.0.0-rc.1",
    "react-dom": "^19.0.0-rc.1",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "eslint-plugin-react-compiler": "^19.0.0-beta-df7b47d-20241124",
    "typescript": "^5.7.2"
  }
}
